<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:zcml="http://namespaces.zope.org/zcml"
    >

  <!-- Finalize all Settings Schemas -->
  <zcml:customAction
      handler=".finalizeSettingsSchemas"
      order="9999999"
      />

  <!-- Plugin: Default permissions for site settings attributes -->
  <adapter
      factory=".plugin.SettingsPermissionsPlugin"
      name="plone.sitesettings.permissions"
      />


</configure>
